{
  "amqp": {
    "type": "amqp",
    "url": "amqp://localhost",
    "exchange": {
      "type": "topic",
      "name": "seneca.topic",
      "options": {
        "durable": true,
        "autoDelete": false
      }
    },
    "client": {
      "channel": {
        "prefetch": 1
      },
      "queues": {
        "prefix": "seneca",
        "separator": ".",
        "options": {
          "durable": true
        }
      }
    },
    "listen": {
      "channel": {
        "prefetch": 1
      },
      "queues": {
        "prefix": "seneca.res",
        "separator": ".",
        "options": {
          "autoDelete": true,
          "exclusive": true
        }
      }
    }
  }
}
